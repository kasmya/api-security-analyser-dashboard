{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": "static",
  "framework": "fastapi",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py/$1"
    }
  ]
}

